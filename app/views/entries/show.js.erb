App.Entry.activeContent = function(){
  var contentSelector = $('<%="#entry-#{@entry.id}"%>')
  App.Entry.hideActiveContent(function(){
    if (contentSelector.length == 0){
      $('#entryContents').append('<%= j render 'show' %>')
    }else{
      contentSelector.addClass('in').addClass('active')
    }
  });
}

if (App.Entry.isEditing() ){
  $.get(App.Entry.cancelEditPath()).success(function(){App.Entry.activeContent()})
}else{
  App.Entry.activeContent()
}
